{
  "name": "screenshot-tweet",
  "version": "1.0.4",
  "description": "Screenshot a Twitter tweet.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": "lukehorvat/screenshot-tweet",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist -q",
    "prebuild": "node_modules/.bin/rimraf dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "nightmare": "2.8.1",
    "tildify": "1.2.0",
    "twitter-url-parser": "0.0.1",
    "yargs": "4.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "rimraf": "2.5.4"
  },
  "keywords": [
    "twitter",
    "tweet",
    "screenshot",
    "capture",
    "image"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
